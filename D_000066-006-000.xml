            <pb facs="" n="1"/>
            <p xml:id="uuid_44ddd341-d8c1-4073-906d-4002e1d69be9">
                <lb xml:id="uuid_1b0484de-3c49-41b4-8484-de3c49f1b493"/>Eingelangt am 11. FEB 1927
                    <lb xml:id="uuid_df5ef352-00ca-47ed-9ef3-5200cae7edae"/>Vr XXVI 715/27 </p>
            <p xml:id="uuid_44557467-9610-4daa-af58-9df5c3429f15">
                <lb xml:id="uuid_9c6af773-40b9-4a7e-aaf7-7340b90a7e7d"/>An das <lb xml:id="uuid_4d9211b3-c626-4974-9211-b3c626497440"/>
